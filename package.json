{
  "name": "test-intern-project",
  "version": "0.0.1",
  "devDependencies": {
    "dojo-loader": "2.0.0-beta.7",
    "intern": "bryanforbes/intern#typings",
    "npm-run-all": "3.1.1",
    "rimraf": "2.5.4",
    "tslint": "3.15.1",
    "typescript": "2.0.9"
  },
  "dependencies": {
    "ts-helpers": "1.1.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "run-p clean:coverage clean:dist",
    "clean:coverage": "rimraf ./html-report",
    "clean:dist": "rimraf ./dist",
    "pretest:unit": "run-s build",
    "test:unit": "intern-client config=dist/tests/intern reporters=Console reporters=LcovHtml"
  }
}
